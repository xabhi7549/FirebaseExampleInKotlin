<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.site_valley.firebaseexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseExample">
        <activity
            android:name=".database.DatabaseInsertDeleteActivity"
            android:label="@string/database" />
        <activity
            android:name=".database.DatabaseActivity"
            android:label="@string/realtime_database" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>